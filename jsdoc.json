{
    "plugins": ["node_modules/better-docs/component", "node_modules/better-docs/category"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": ["./src"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "better-docs": {
            "name": "Encryption tool"
        }
    },
    "opts": {
        "template": "node_modules/better-docs",
        "recurse": true,
        "destination": "docs/",
        "readme": "README.md"
    }
}